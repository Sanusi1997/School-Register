@page
@model StudentRecord.Pages.Registers.ListModel
@{
    ViewData["Title"] = "Student Record Page";
}
<h1>Register</h1>

<form method="get">
    <div class="form-group">
        <div class="input-group">
            <input type="search" class="form-control" asp-for="SearchTerm" />
            <span class="input-group-append">
                <button class="btn" type="submit">
                    <ion-icon name="search-sharp"></ion-icon>
                </button>
            </span>
        </div>
    </div>
</form>
@foreach (var register in Model.Registers)
{
    <partial name="_Summary" model="register" />
}
<br />
<a asp-page="./Edit" class="btn btn-primary">Add New</a>
@section footer{
    <div class="border-primary">@Model.Message</div>
}